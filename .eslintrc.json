{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "max-len": ["error", 200],
    "class-methods-use-this": 0,
    "import/extensions": 0,
    "no-param-reassign":0,
    "import/no-unresolved":0,
    "no-underscore-dangle":0,
    "import/prefer-default-export": 1
  }
}
